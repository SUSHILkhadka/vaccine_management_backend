{
  "name": "vaccine-management",
  "version": "1.0.0",
  "description": "System that maintains vaccine and allergies related to vaccine",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "build": "tsc",
    "postinstall": "tsc",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*.{ts,js}' 'test/**/*.{ts,js}'",
    "prettify": "prettier --single-quote --print-width 120 --write {src,test}/**/*.ts",
    "format:code": "run-s lint:fix prettify",
    "migrate:make": "knex migrate:make --migrations-directory src/db/migrations -x ts",
    "migrate": "knex migrate:latest --knexfile src/db/knexfile.ts",
    "migrate:prod": "knex migrate:latest --knexfile dist/db/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile src/db/knexfile.ts",
    "git": "npm run prettify && git add . && git commit -m",
    "git:rename": "npm run prettify && git add . && git commit --amend -m"
  },
  "keywords": [
    "typescript",
    "nodejs"
  ],
  "dependencies": {
    "babel-jest": "^29.3.1",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.30.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.2.0",
    "knex-stringcase": "^1.4.6",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.7.3",
    "sqlite3": "^5.0.11",
    "ts-jest": "^29.0.3",
    "winston": "^3.8.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.2",
    "jest": "^29.3.1",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-lint": "^4.5.1",
    "ts-node": "^10.9.1",
    "tslint-config-leapfrog": "^1.0.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "author": "Sushil Khadka <shusilkhadka2@gmail.com>"
}
